{
  "name": "final-affiliate-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit && npm run type-check",
    "docker:build": "docker build -t be4youdeals .",
    "docker:run": "docker run -p 3000:3000 be4youdeals",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up -d"
  },
  "dependencies": {
    "@sentry/nextjs": "10.12.0",
    "autoprefixer": "^10.4.14",
    "firebase": "^10.14.1",
    "firebaseui": "^6.1.0",
    "framer-motion": "^12.23.5",
    "next": "13.4.19",
    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.5.2",
    "swiper": "^11.2.10",
    "tailwindcss": "^3.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.13.3",
    "@types/node": "24.0.10",
    "@types/react": "19.1.8",
    "typescript": "5.8.3"
  },
  "netlify": {
    "plugins": [
      "@netlify/plugin-nextjs"
    ]
  }
}
